{
  "name": "Hello World!",
  "description": "My first Chrome App.",
  "version": "0.1",
  "manifest_version": 2,
    "background": {
    "persistent": false,
      "scripts": ["background.js"]
    },
     "permissions": [
    "tabs"
  ],
   "content_scripts": [{
        "matches":    ["*://play.google.com/*"],
        "js":         ["content.js"],
        "run_at": "document_end"
    }],
  "browser_action": {
    "title": "Test"
  },
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}